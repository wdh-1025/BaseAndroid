<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wdh.base">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".app.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ListActivity" />
        <activity android:name=".activity.TabLayoutActivity" />
        <activity android:name=".activity.MDDialogActivity" />
        <activity android:name=".activity.PhotoViewActivity"/>
        <activity android:name=".activity.LinkCallTaskActivity"/>
        <activity android:name=".activity.BannerActivity"/>
        <activity android:name=".activity.ViewActivity"/>
        <activity android:name=".activity.OKHttpActivity"/>
        <activity android:name=".activity.WebViewJsBridge"/>
        <activity android:name=".activity.ButtonActivity"/>
        <activity android:name=".activity.QrCodeActivity"/>
    </application>

</manifest>